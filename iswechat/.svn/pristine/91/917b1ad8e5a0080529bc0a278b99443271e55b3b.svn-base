<?php
namespace Home\Model;

use Think\Model;

class QuestionModel extends Model {
    protected $tablePrefix = 'cms_';
    protected $tableName = 'exam_question';



    public function getQuestion2Key($key = null)
    {

        foreach ($key as $k){
            $where[]  = " LIKE '%{$k}%'";
        }


        $data = M('cms_exam_question')->where($where)->find('');

        printR($data);
    }


}