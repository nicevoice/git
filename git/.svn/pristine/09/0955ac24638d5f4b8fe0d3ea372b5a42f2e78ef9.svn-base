	margin-left:10px;
	line-height:20px;
}
.content-vote-shuimo .input-radio{margin-right:4px;}
.content-vote-shuimo  label {
	cursor:pointer;
}
 
 .content-vote-shuimo .content-vote-shuimo {margin-top:10px;margin-bottom:10px;float:left;}
 .content-vote-shuimo  a.result{margin-left:10px; color:#c00; text-decoration:none;}
 .dd-submit {margin-top:10px;}