.content-vote-shuimo{background-color:#FFF; font-size:12px; color:#663300;}
.content-vote-shuimo .mod-vote {
	padding:10px;
	padding:10px;
	overflow:hidden;
	zoom:1;
}
.content-vote-shuimo  dl {
	margin:3px 0;
}
.content-vote-shuimo  dt {
	color:#663300;
	line-height:20px;
}
