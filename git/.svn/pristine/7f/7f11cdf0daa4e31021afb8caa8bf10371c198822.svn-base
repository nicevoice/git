 /**
  * 动画
  */
@-webkit-keyframes fadeIn {
	0% {opacity: 0.5;}	
	100% {opacity: 1;}
}
@-webkit-keyframes fadeOut {
	0% {opacity: 0.5;}	
	100% {opacity: 0;}
}
@-webkit-keyframes leftToHidden {
	0% {
		-webkit-transform: translateX(0);
	}
	100% {
		-webkit-transform: translateX(-100%);
	}
}
@-webkit-keyframes RightToShow {
	0% {
		-webkit-transform: translateX(100%);
	}
	100% {
		-webkit-transform: translateX(0);
	}
}
@-webkit-keyframes android_leftToHidden {
	0% {
		left:0;
	}
	100% {
		left:-100%;
	}
}
@-webkit-keyframes android_RightToShow {
	0% {
		left:100%;
	}
	100% {
		left: 0;
	}
}
@-webkit-keyframes InRight {
	0% {
		-webkit-transform: translateX(100%);
	}
	100% {
		-webkit-transform: translateX(0);
	}
}
@-webkit-keyframes InLeft {
	0% {
		-webkit-transform: translateX(0);
	}
	100% {
		-webkit-transform: translateX(-100px);
	}
}
@-webkit-keyframes InRight2 {
	0% {
		-webkit-transform: translateX(100px);
	}
	100% {
		-webkit-transform: translateX(0);
	}
}
@-webkit-keyframes InLeftBackShow {
	0% {
		-webkit-transform: translateX(-100%);
	}
	100% {
		-webkit-transform: translateX(0);
	}
}
@-webkit-keyframes InLeftBackHide {
	0% {
		-webkit-transform: translateX(0);
	}
	100% {
		-webkit-transform: translateX(100%);
	}
}
@-webkit-keyframes pulse {
    0% { -webkit-transform: scale(1); }	
	50% { -webkit-transform: scale(1.1); }
    100% { -webkit-transform: scale(1); }
}
@-webkit-keyframes rollIn {
	0% { opacity: 0; -webkit-transform: translateX(-100%) rotate(-120deg); }
	100% { opacity: 1; -webkit-transform: translateX(0px) rotate(0deg); }
}
