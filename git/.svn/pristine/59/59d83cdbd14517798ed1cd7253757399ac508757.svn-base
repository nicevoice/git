<?xml version="1.0" encoding="UTF-8"?>
<root> 
 	<name tips="请输入任务名称">
	  	<required event="submit">名称不能为空</required>
	  	<min args="2">名称不能少于2个字</min>
	   	<max args="50">名称不能大于50个字</max>
    </name>
    <alias tips="只能有字母数字和下划线">
	  	<required event="submit">别名不能为空</required>
	  	<regex args="#^[\w]+$#">别名只能是字母开头,可包含字母数字及下划线.</regex>
    </alias>
    <url tips="页面的URL,http开头">
    	<required event="submit">页面的网址不能为空</required>
	  	<regex args="#^https?\:\/\/.*?$#">只能是http开头的有效网址</regex>
    </url>
</root>