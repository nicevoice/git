<?php
namespace sWechat;


session_start();
//引入配置文件
include_once __DIR__.'/config.inc.php';


//引入自动载入函数
include_once __DIR__.'/autoloader.php';
//调用自动载入函数
AutoLoader::register();



/**
 * 格式化打印数据
 * @param $data
 * @param bool $die
 */
function printR($data, $die = true)
{
    header("Content-type: text/html; charset=utf-8");
    echo '<pre>';
    print_r($data);
    if($die)
    {
        die();
    }
}